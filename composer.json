{
  "name": "fullsmack/livewire-slice",
  "description": "Registration of Livewire components for Laravel Slice",
  "homepage": "https://github.com/fullsmack/livewire-slice",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "nicolaibaaring"
    }
  ],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
    "livewire/livewire": "^3.0",
    "fullsmack/laravel-slice": "dev-main|dev-development"
  },
  "autoload": {
    "psr-4": {
      "FullSmack\\LivewireSlice\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FullSmack\\LivewireSlice\\LivewireSliceServiceProvider"
      ]
    }
  }
}
